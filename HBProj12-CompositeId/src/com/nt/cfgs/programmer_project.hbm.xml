<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
    <class name="com.nt.domain.Programmer_Project" table="Programmer_Project" >

	<composite-id name="id"  class="com.nt.cid.Programmer_ProjectId">
	<key-property name="programmerId"/>
	<key-property name="projectId" />
	</composite-id>
	<property name="programmerName"/>
	<property name="projectName"/>
	</class>

		

	
</hibernate-mapping>
